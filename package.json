{
  "name": "rc-whitelabel",
  "version": "15.0.0",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "rdl": "npm run cert & ng serve --port 54747",
    "camc": "ng serve --port 8006",
    "ssl": "npm run cert & ng serve --port",
    "cert": "mkdir -p ./tls & mkcert -cert-file ./tls/localhost-cert.pem -key-file ./tls/localhost-key.pem -ecdsa localhost 127.0.0.1 ::1",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --configuration production --aot"
  },
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/google-maps": "^15.0.3",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "path": "^0.12.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
