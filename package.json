    "@angular/compiler-cli": "^15.0.4",
    "@types/node": "^16.18.10",
    "codelyzer": "^6.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
